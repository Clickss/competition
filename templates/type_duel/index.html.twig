{% extends 'base.html.twig' %}

{% block title %}TypeMatch index{% endblock %}

{% block body %}
    <h1>TypeMatch index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type_match</th>
                <th>Code_type_match</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_match in type_matches %}
            <tr>
                <td>{{ type_match.id }}</td>
                <td>{{ type_match.typeMatch }}</td>
                <td>{{ type_match.codeTypeMatch }}</td>
                <td>
                    <a href="{{ path('type_match_show', {'id': type_match.id}) }}">show</a>
                    <a href="{{ path('type_match_edit', {'id': type_match.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('type_match_new') }}">Create new</a>
{% endblock %}